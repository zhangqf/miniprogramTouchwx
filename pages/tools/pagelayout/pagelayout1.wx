

<template>

    <view>
         <view wx:if="{{_ShowEmpSubscribeImage}}" bindtap="cancelShowEmpSubscribeImage" style="position:absolute;background:rgba(33,33,33,.4);width:100%;height:100%; z-index:11;top:0;left:0">
            <image src="{{_empInfo.subscribeImageURL}}"  style="position: flex;left: 0;right: 0;margin:15% auto;top: 13%;height:667rpx;width:680rpx;" />
        </view>
<import src="../../plugin/wxParse/wxParse.wxml"/>
        <view class="navigate" wx:for="{{_pageLayoutData.listNavigate}}" wx:for-item="navigateItem" wx:for-index="navigateIndex" wx:key="navigate">
            <view  style="position:relative; overflow:auto;" wx:if="{{navigateIndex==_activeNavigateIndex}}" >

                <view  wx:for="{{navigateItem.listModule}}" wx:for-item="moduleItem"  wx:for-index="moduleIndex" wx:key="module" >
                    
                    <!-- 会员卡 -->
                            <block  >
                                <view class="ui-content" wx:if="{{moduleItem.moduleEmpData}}" style="margin-bottom:24rpx;position:relative;">
                                    <view class="personal" style="background-image:url({{_empInfo.EmpLevelImageURL!=null?_empInfo.EmpLevelImageURL:_empInfo.ImageURL}});background-repeat: no-repeat;background-size:100%" >
                                        <block wx:if="{{_empStatusRegister}}">
                                            <view class="personal-title">
                                                <image class="image" data-url="{{moduleItem.moduleEmpData.clickLogoJumpURL}}" bindtap="clickEmpLogo"  src="{{_empInfo.avatarUrl==null?_empInfo.logo:_empInfo.avatarUrl}}"/>
                                                <view class="ui-radius">
                                                    <text class="person-emName">{{_empInfo.empAlias}}</text>
                                                    <view wx:if="{{moduleItem.moduleEmpData.isShowEmpLevel}}" class="gold-membership-card">{{_empInfo.empLevelDisplayName}}</view>
                                                    <view class="ui-phoneitems">
