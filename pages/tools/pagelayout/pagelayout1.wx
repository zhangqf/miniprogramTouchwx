                                                        <text class="person-phoneNum" wx:if="{{moduleItem.moduleEmpData.isShowPhone && _empInfo.phone}}">{{moduleItem.moduleEmpData.isShowPhoneFilter==false?_empInfo.phone:_empInfo.phoneFilter}}</text>
                                                    </view>
                                                </view>
                                                <image wx:if="{{moduleItem.moduleEmpData.isShowEmpQRCode}}" bindtap="linkToEmpQRcode" class="person-qrcode" src="../../images/QRcode.png"/>
                                            </view>
                                            <view class="ui-text" style="color:{{moduleItem.moduleEmpData.empNameColor}}" wx:if="{{moduleItem.moduleEmpData.isShowEmpName}}">{{_empInfo.empName}}</view>
                                            <view class="ui-personalitems">
                                                <view wx:if="{{moduleItem.moduleEmpData.isShowPerfectEmpInfo && _empInfo.isShowPerfectEmpInfo}}" 
                                                bindtap="linkToPerfectEmp" class="ui-personalitem">
