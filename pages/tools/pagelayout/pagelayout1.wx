                                            </view>
                                          
                                        </block>
                                        <block wx:else>
                                            <view class="personal-title">
                                                <image class="image"  src="{{_empInfo.avatarUrl==''?_empInfo.Logo:_empInfo.avatarUrl}}"/>
                                                <view class="ui-radius">
                                                    <text class="person-emName">{{_empInfo.empAlias!=''?_empInfo.empAlias:_empInfo.title}}</text>
                                                </view>
                                            </view>
                                              <view class="ui-button"  wx:if="{{_canIUse&&!_empStatusGrant}}">
