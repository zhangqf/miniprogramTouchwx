                                              </view>
                                                <view class="ui-button"  wx:if="{{_empStatusGrant}}">
                                                    <button open-type="getPhoneNumber"  lang="zh_CN" bindgetphonenumber="registerEmp">开通会员</button>
                                              </view>
                                        </block>
                                        
                                    </view>
                                    <view class="emp-item">
                                        <view  wx:for="{{moduleItem.moduleEmpData.listItem}}" wx:for-item="empitem" wx:for-index="empIndex" wx:key="key" wx:if="{{empIndex<=3}}">
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_Balance}}">
                                                <view class="ui-empitemsNo">{{_empInfo.empMoney==null?"-":_empInfo.empMoney}}</view>
                                                <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>储值</view>
                                            </view>
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_Point}}">
                                                  <view class="ui-empitemsNo">{{_empInfo.empPoint==null?"-":_empInfo.empPoint}}</view>
                                              <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>积分</view>
                                            </view>
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_Growth}}">
                                                <view class="ui-empitemsNo">{{_empInfo.empGrowth==null?"-":_empInfo.empGrowth}}</view>
                                                <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>成长值</view>
                                            </view>
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_EmpLevel}}">
                                                <view class="ui-empitemsNo">{{_empInfo.empLevelDisplayName==null?"-":_empInfo.empLevelDisplayName}}</view>
                                                <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>等级</view>
                                            </view>
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_CouponNum}}">
                                                <view class="ui-empitemsNo">{{_empInfo.couponNum==null?"-":_empInfo.couponNum}}</view>
                                                <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>优惠券</view>
                                            </view>
                                            <view class="ui-empitems" wx:if="{{empitem.itemType==_enumData.PageLayoutNavigateModuleEmpItemTypeEnum_Custom}}">
                                                <view class="ui-empitemsNo">{{empitem.customValue}}</view>
                                                <view class="ui-empitemstitle"><image wx:if="{{empitem.icon!=''&&empitem.icon!='undefined'&&empitem.icon!=null}}" src="{{empitem.icon}}" alt/>{{empitem.title}}</view>
                                            </view>
                                        </view>
                                    </view>
                                    
                                </view>
                            </block>
                    <!-- 轮播图 -->
                            <block wx:if="{{moduleItem.moduleBannerData}}">
                                <view class="ui-content" style="padding:24rpx 0; ">
                                    <swiper class="three_swiper" current="0" indicator-color="rgba(51, 51, 51, 0.3)" indicator-active-color="#ff2a00"   
                                    autoplay=true interval="{{moduleItem.moduleData.interval}}" autoHeight=true style="height:{{moduleItem.moduleData.bannerHeight}}px"
                                    circular=true easing-function="easeInOutCubic">
                                        <swiper-item wx:for="{{moduleItem.moduleData.pages}}" wx:for-item="bannerPageItem" wx:key="bannerPage">
                                            <view class="swiper_item" bindtap="goBannerLink" data-url="{{bannerPageItem.url}}">
                                                <image data-id='{{index}}'  src="{{bannerPageItem.imageURL}}" mode='widthFix'/>
                                                <view class="ht-bannerTitle">{{bannerPageItem.title}}</view>
                                            </view>
                                        </swiper-item>
                                    </swiper>
                                </view>
                            </block>
                        <!-- 图文 -->
                            <block wx:if="{{moduleItem.moduleContentData}}">
                                <view class="ui-content ui-imageContent" >
                                  
                                <!-- //这里data中article为bindName -->
                                <template is="wxParse" data="{{wxParseData:[moduleItem.moduleContentData.ContentName].nodes}}"></template>
                                </view>
                            </block>
                        <!-- 图片 -->
                            <block wx:if="{{moduleItem.moduleImageData}}">
                                <view class="ui-content">
                                  <image class="ui-picImage" mode="widthFix" src="{{moduleItem.moduleImageData.imageURL}}" lazy-load=true show-menu-by-longpress=true data-url="{{moduleItem.moduleImageData.linkURL}}"
                                  data-linkJumpType="{{moduleItem.moduleImageData.linkJumpType}}" bindtap="goImageLink"/>
                                </view>
                            </block>
                    <!-- 功能项 -->
                            <block wx:if="{{moduleItem.moduleFunctionData}}">
                                <!-- 九宫格 -->
                               
                               

                                <view class="ui-content ui-full" wx:if="{{moduleItem.moduleFunctionData.functionType==10}}" style="background:{{moduleItem.moduleFunctionData.backgroundColor}}">
                                    <view class="ui-linetitle">{{moduleItem.moduleFunctionData.title}}</view>
                                   <view  wx:for="{{moduleItem.moduleFunctionData.listItem}}" wx:for-item="functionItem" wx:for-index="functionIndex" wx:key="key">
                                 
                        <ui-row class="ui-entry" wx:if="{{functionIndex % moduleItem.moduleFunctionData.showItemNumByRow == 0}}" border-top style="background:{{moduleItem.moduleFunctionData.backgroundColor}};justify-content:initial">
                        <ui-col wx:for="{{moduleItem.moduleFunctionData.listItem}}" 
                        wx:for-item="functionColItem" 
                        wx:for-index="functionColIndex"  
                        wx:key="key"
                        wx:if="{{functionColIndex >= functionIndex && functionColIndex < functionIndex + moduleItem.moduleFunctionData.showItemNumByRow}}" 
                        vertical-align="middle"
                        class="ui-col {{(functionColIndex+1)%moduleItem.moduleFunctionData.showItemNumByRow!=0?'ui-borderRight':'ui-borderNone'}}" 
                        style="width:{{(718-moduleItem.moduleFunctionData.showItemNumByRow-1)/moduleItem.moduleFunctionData.showItemNumByRow}}rpx;flex:none"> 
                            <view bindtap="goFucntionLink" hover-class="navigator-hover" class="btnitems"  
                                            data-jumpType="{{functionColItem.jumpType}}"
                                            data-linkJumpType="{{functionColItem.linkJumpType}}"  data-url="{{functionColItem.linkURL}}" 
                                            data-miniProgramAppID ="{{functionColItem.miniProgramAppID}}" data-miniProgramPath ="{{functionColItem.miniProgramPath}}" 
                                            data-phone="{{functionColItem.phone}}" 
                                            data-isNew="{{functionColItem.isNew}}" data-isHot="{{functionColItem.isHot}}" data-isOnlyEmp="{{functionColItem.isOnlyEmp}}" 
                                            data-isComingSoon="{{functionColItem.isComingSoon}}" data-comingSoonText="{{functionColItem.comingSoonText}}" >
                                                <!-- <span class="ui-icons icon-xinyongqia ui-size"></span> -->
                                                <view wx:if="{{functionColItem.isOnlyEmp}}" class="ui-myiconvip"><image src="../../images/vip.png" /></view>
                                                <view wx:if="{{functionColItem.isHot}}" class="ui-myiconhot" ><image src="../../images/listhot.png" /></view>
                                                <view wx:if="{{functionColItem.isComingSoon}}" class="ui-myiconwait">
                                                    <image src="../../images/comingsoon.png" />
                                                </view>
                                                    <view wx:if="{{functionColItem.isNew}}" class="ui-myiconnew" ><image src="../../images/newright.png" /></view>
                                                <image wx:if="{{functionColItem.icon!=''&&functionColItem.icon!=null&&functionColItem.icon!='undefined'}}" src="{{functionColItem.icon}}" class="ui-entryImage" />
                                                <text class="ui-clotext">{{functionColItem.title}}</text>
                                            </view>                           
                        
                        </ui-col>
                        </ui-row>

                                </view>
                                </view>
                                <!-- 单行 -->
                              <view class="ui-lineScroll" wx:if="{{moduleItem.moduleFunctionData.functionType==20}}" style="background:{{moduleItem.moduleFunctionData.backgroundColor}}">
                                    <view class="ui-linetitle">{{moduleItem.moduleFunctionData.title}}</view>
                                    <scroll-view scroll-x="true" style=" white-space: nowrap; display: flex ;flex-direction: row; enable-flex:true;background: #ffff;height:200rpx;">
                                            <view class="ui-lineScrollContent {{moduleItem.moduleFunctionData.isShowGridLine==true?'ui-borderRight':'ui-borderNone'}}" wx:for="{{moduleItem.moduleFunctionData.listItem}}" wx:for-item="lineitem" wx:key="key" 
                                            
                                            >
                                                <view class="ui-lineScrollContentDisplayFlex" bindtap="goFucntionLink" data-url="{{lineitem.linkURL}}"
                                    data-jumpType="{{lineitem.jumpType}}" data-linkJumpType="{{lineitem.linkJumpType}}" data-appid = "{{lineitem.miniProgramAppID}}" data-phone="{{lineitem.phone}}"
                                    data-isNew="{{lineitem.isNew}}" data-isHot="{{lineitem.isHot}}" data-isOnlyEmp="{{lineitem.isOnlyEmp}}" 
                                            data-isComingSoon="{{lineitem.isComingSoon}}" data-comingSoonText="{{lineitem.comingSoonText}}" hover-class="bg_ccc"
                                    >
                                                    <view class="ui-myiconvip" wx:if="{{lineitem.isOnlyEmp}}"><image src="../../images/vip.png" /></view>
                                                        <view class="ui-myiconhot" wx:if="{{lineitem.isHot}}"><image src="../../images/listhot.png" /></view>
                                                        <view class="ui-myiconwait"  wx:if="{{lineitem.isComingSoon}}">
                                                            <image src="../../images/comingsoon.png" />
                                                        </view>
                                                    <image  wx:if="{{lineitem.icon!=''&&lineitem.icon!=null&&lineitem.icon!='undefined'}}" src="{{lineitem.icon}}"  />
                                                    <text class="ui-clotext">{{lineitem.title}}</text>
                                                </view>
                                            </view>
                                          
                                    </scroll-view>
                                </view>
                                <!-- 多行 -->
                                <view class="ui-contentline top_tip"  wx:if="{{moduleItem.moduleFunctionData.functionType==30}}" style="background:{{moduleItem.moduleFunctionData.backgroundColor}}">
                                    <view class="ui-linetitle">{{moduleItem.moduleFunctionData.title}}</view>
                                    <ui-row height="55" border-bottom hover-class="touchui-hover" wx:for="{{moduleItem.moduleFunctionData.listItem}}" wx:for-item="custom_list" wx:key="key" bindtap="goFucntionLink" data-url="{{custom_list.linkURL}}"
                                    data-jumpType="{{custom_list.jumpType}}" data-linkJumpType="{{custom_list.linkJumpType}}"
                                    data-appid = "{{custom_list.miniProgramAppID}}" data-phone="{{custom_list.phone}}"
                                      data-isNew="{{custom_list.isNew}}" data-isHot="{{custom_list.isHot}}" data-isOnlyEmp="{{custom_list.isOnlyEmp}}" 
                                    data-isComingSoon="{{custom_list.isComingSoon}}" data-comingSoonText="{{custom_list.comingSoonText}}"
                                    >
                                        <ui-col width="44" align="center" vertical-align="middle">
                                            <view class="left_icon">
                                                <image wx:if="{{custom_list.icon!=''&&custom_list.icon!=null&&custom_list.icon!='undefined'}}" src="{{custom_list.icon}}" />
                                            </view>
                                        </ui-col>
                                        <ui-col class="text" align="left" vertical-align="middle" >
                                            <view style="width: 100%;">
                                                <ui-row >
                                                    <ui-col align="left" vertical-align="middle" class="ui-middle" >
                                                      <view class="ui-linemiddle">
                                                        <text>{{custom_list.title}}</text>
                                                        <image wx:if="{{custom_list.isNew}}" class="line-new" src="../../images/newleft.png" /> 
                                                          <image wx:if="{{custom_list.isHot}}" class="line-hot" src="../../images/listhot.png" />
                                                        <image wx:if="{{custom_list.isOnlyEmp}}" class="line-vip" src="../../images/listvip.png" />
                                                        <image wx:if="{{custom_list.isComingSoon}}" class="line-comingsoon" src="../../images/comingsoon.png" />
                                                      </view>
                                                    </ui-col>
                                                    <ui-col align="right" width="100" vertical-align="middle" space-right="10" >
                                                      <view class="ui-left" >
                                                        <text>{{custom_list.description}}</text>
                                                        <image class="ui-leftarrow" src="../../images/leftarrow.png"/>
                                                      </view>
                                                    </ui-col>
                                                </ui-row>
                                              
                                            </view>
                                        </ui-col>
                                    </ui-row>
                                </view>
                            </block>
              
                </view>
                
                <view style="height:{{NAV_HEIGHT}}; background:#f7f7f7;" ></view>
            </view>
        </view>
        <ui-fixed-view bottom="0" left="0" right="0" wx:if="{{_pageLayoutData.listNavigate.length>1}}" >
            <view class="fixed-view-content" >
                <view class="fixed-items" style="background:{{_pageLayoutData.navigateBackgroundColor}}"
                wx:for="{{_pageLayoutData.listNavigate}}" wx:for-item="navigateTabItem"  wx:key="navigate" data-num="{{index}}" data-isOnlyEmp="{{navigateTabItem.isOnlyEmp}}"
                bindtap="btntarbar">
                    <image class="tarimg" src="{{index==_activeNavigateIndex?navigateTabItem.selectIcon:navigateTabItem.icon}}"/>
                    <view class="displayname" style="color:{{index==_activeNavigateIndex?_pageLayoutData.navigateTitleSelectColor:_pageLayoutData.navigateTitleColor}};">{{navigateTabItem.title}}</view>
                </view>
                <!-- <view class="fixed-items" hover-class="btn_hover_list">c</view> --> 
            </view>
        </ui-fixed-view>
